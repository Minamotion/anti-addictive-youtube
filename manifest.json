{
	"manifest_version": 2,
	"name": "Anti-Addictive YouTube",
	"version": "0.1",
	"description": "A simple extension that doesn't let YouTube load more videos when you reach the bottom of their Homepage",
	"icons": {
		"16": "icon.svg",
		"19": "icon.svg",
		"24": "icon.svg",
		"32": "icon.svg",
		"38": "icon.svg",
		"48": "icon.svg",
		"64": "icon.svg",
		"96": "icon.svg",
		"128": "icon.svg"
	},
	"content_scripts": [
		{
			"matches": [
				"*:\/\/*.youtube.com/*"
			],
			"js": [
				"extension.js"
			]
		}
	]
}